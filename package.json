{
  "name": "storybook",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://janganacode.github.io/react-storybook",
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "deploy-storybook": "storybook-to-ghpages",
    "start": "react-scripts start",
    "storybook-start": "start-storybook -p 4000 -c .storybook",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "build-storybook -c .storybook -o .out"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "description": "Storybook for ReactJS components",
  "devDependencies": {
    "@storybook/react": "^4.0.8",
    "@storybook/storybook-deployer": "^2.3.0"
  }
}
